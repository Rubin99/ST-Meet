<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stmeet">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.STMeet">
        <activity android:name=".TeacherInfoActivity"></activity>
        <activity android:name=".SubjectListActivity" />
        <activity android:name=".chat.ChatActivity" />
        <activity android:name=".matches.MatchesActivity" />
        <activity android:name=".info.UserInfoActivity" />
        <activity android:name=".login_registration.LoginActivity" />
        <activity android:name=".login_registration.RegistrationActivity" />
        <activity android:name=".login_registration.ChooseLoginRegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>